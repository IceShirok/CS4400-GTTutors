<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.net.URL?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.text.Text?>

<GridPane alignment="CENTER" hgap="10.0" styleClass="root" vgap="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="edu.gatech.GTTutors.controller.FindController">
  <Label fx:id="message" GridPane.columnIndex="0" GridPane.rowIndex="2" />
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <padding>
    <Insets bottom="10.0" left="25.0" right="25.0" top="25.0" />
  </padding>
  <rowConstraints>
    <RowConstraints maxHeight="16.0" minHeight="10.0" prefHeight="16.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="16.0" minHeight="10.0" prefHeight="16.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="20.0" minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="100.0" minHeight="10.0" prefHeight="100.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="20.0" minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
  </rowConstraints>
  <stylesheets>
    <URL value="@../Login.css" />
  </stylesheets>
  <HBox alignment="BOTTOM_RIGHT" spacing="10.0" GridPane.columnIndex="0" GridPane.rowIndex="4">
    <children>
      <Button mnemonicParsing="false" onAction="#goBack" text="Back" />
    </children>
  </HBox>
  <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="3">
    <children>
      <TableView fx:id="findTable" prefHeight="150.0" prefWidth="511.0">
        <columns>
          <TableColumn prefWidth="75.0" text="Day" fx:id="day" />
          <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="70.0" text="Time" fx:id="time" />
          <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="65.0" text="Name" fx:id="name" />
          <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="109.0" text="Email" fx:id="email" />
          <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="79.0" text="Course" fx:id="course" />
        </columns>
      </TableView>
    </children>
  </HBox>
  <Button mnemonicParsing="false" onAction="#submit" text="OK" GridPane.columnIndex="0" GridPane.rowIndex="2" />
  <Label id="title" text="Find Tutor Schedule" GridPane.columnIndex="0" GridPane.rowIndex="0" />
  <HBox prefHeight="100.0" prefWidth="500.0" GridPane.columnIndex="0" GridPane.rowIndex="1">
    <children>
      <Label text="Enter Tutor GTID:" />
      <TextField fx:id="gtid" prefWidth="200.0" />
    </children>
  </HBox>
</GridPane>
