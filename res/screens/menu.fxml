<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.net.URL?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.text.Text?>

<GridPane alignment="CENTER" hgap="10.0" styleClass="root" vgap="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="src.code.controller.MenuController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <padding>
    <Insets bottom="10.0" left="25.0" right="25.0" top="25.0" />
  </padding>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
  <stylesheets>
    <URL value="@../Login.css" />
  </stylesheets>
  <HBox alignment="BOTTOM_RIGHT" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="9">
    <children>
      <Button mnemonicParsing="false" onAction="#goBack" text="Logout" />
    </children>
  </HBox>
  <Text fx:id="loginInfo" text="Logged in as:" GridPane.columnIndex="1" GridPane.rowIndex="0" />
  <HBox alignment="CENTER" prefHeight="29.0" prefWidth="500.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
    <children>
      <Button fx:id="search" mnemonicParsing="false" onAction="#submit" text="Search for Tutors" />
      <Button fx:id="schedule" mnemonicParsing="false" onAction="#submit" text="Schedule a Tutor" />
      <Button fx:id="rate" onAction="#submit" text="Rate a Tutor" />
    </children>
  </HBox>
  <HBox alignment="CENTER" prefHeight="29.0" prefWidth="500.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
    <children>
      <Button fx:id="apply" mnemonicParsing="false" onAction="#submit" text="Apply" />
      <Button fx:id="find" mnemonicParsing="false" onAction="#submit" text="Look Up Schedule" />
    </children>
  </HBox>
  <HBox alignment="CENTER" prefHeight="29.0" prefWidth="500.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
    <children>
      <Button fx:id="recommend" mnemonicParsing="false" onAction="#submit" text="Recommend a Tutor" />
    </children>
  </HBox>
  <HBox alignment="CENTER" prefHeight="29.0" prefWidth="500.0" GridPane.columnIndex="1" GridPane.rowIndex="5">
    <children>
      <Button fx:id="sum1" mnemonicParsing="false" onAction="#submit" text="View Summary 1" />
      <Button fx:id="sum2" mnemonicParsing="false" onAction="#submit" text="View Summary 2" />
    </children>
  </HBox>
</GridPane>
