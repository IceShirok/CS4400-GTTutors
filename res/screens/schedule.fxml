<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.net.URL?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.text.Text?>

<GridPane alignment="CENTER" hgap="10.0" styleClass="root" vgap="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="edu.gatech.GTTutors.controller.ScheduleController">
  <Label fx:id="message" GridPane.columnIndex="0" GridPane.rowIndex="1" />
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <padding>
    <Insets bottom="10.0" left="25.0" right="25.0" top="25.0" />
  </padding>
  <rowConstraints>
    <RowConstraints maxHeight="10.0" minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="20.0" minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="20.0" minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="100.0" minHeight="10.0" prefHeight="100.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="20.0" minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
  </rowConstraints>
  <stylesheets>
    <URL value="@../Login.css" />
  </stylesheets>
  <HBox alignment="BOTTOM_RIGHT" spacing="10.0" GridPane.columnIndex="0" GridPane.rowIndex="4">
    <children>
      <Button mnemonicParsing="false" onAction="#goBack" text="Back" />
      <Button mnemonicParsing="false" onAction="#submit" text="OK" />
    </children>
  </HBox>
  <Text id="title" text="Select Your Tutor" GridPane.columnIndex="0" GridPane.rowIndex="0" />
  <Label text="Select a tutor in the table." GridPane.columnIndex="0" GridPane.rowIndex="1" />
  <Separator prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="2" />
  <TableView prefHeight="250.0" prefWidth="511.0" GridPane.columnIndex="0" GridPane.rowIndex="3">
    <columns>
      <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="148.0" text="Name" />
      <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="147.0" text="Email" />
      <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="81.0" text="Day" />
      <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="69.0" text="Time" />
    </columns>
  </TableView>
</GridPane>
